{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": ["Settings for Java dependencies: GraalVM"],
  "java": {
    "packageRules": [
      {
        "description": "Group dependencies",
        "packagePatterns": ["^org\\.graalvm[.:].+"],
        "excludePackagePatterns": ["^org\\.graalvm\\.plugins[.:].+"],
        "groupName": "graalvm"
      }
    ]
  }
}
