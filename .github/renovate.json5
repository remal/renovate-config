{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["github>remal/renovate-config:global"],

  newLogLevel: [
    {
      matchMessage: '/.*/',
      newLogLevel: 'info',
    },
  ],

  packageRules: [
    {
      matchDepNames: ['renovate'],
      //schedule: ["after 6pm"]
      minimumReleaseAge: '2 weeks',
      internalChecksFilter: 'strict',
    },
  ],
}
