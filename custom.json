{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "description": "Custom managers",
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "/\\.properties$/"
      ],
      "matchStrings": [
        "(^|[\\r\\n]+)#[ \\t]*renovate: datasource=(?<datasource>\\S+)( depName=(?<depName>\\S+))?( packageName=(?<packageName>\\S+))?( versioning=(?<versioning>\\S+))?( registryUrl=(?<registryUrl>\\S+))?[ \\t]*[\\r\\n]+[^=\\n]+=[ \\t]*(?<currentValue>\\S+)([\\r\\n]+|$)"
      ]
    }
  ]
}
