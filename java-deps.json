        "java"
      ],
      "matchPackagePatterns": [
        "^com\\.fasterxml\\.jackson[.:].+"
      ],
      "groupName": "com.fasterxml.jackson"
    },


    {
      "description": "jacoco: group dependencies",
      "matchLanguages": [
        "java"
      ],
      "matchCategories": [
        "java"
      ],
      "matchPackagePatterns": [
        "^org\\.jacoco:"
      ],
      "groupName": "org.jacoco"
    },


    {
      "description": "javaparser: group dependencies",
      "matchLanguages": [
        "java"
      ],
      "matchCategories": [
        "java"
      ],
      "matchPackagePatterns": [
        "^com\\.github\\.javaparser:"
